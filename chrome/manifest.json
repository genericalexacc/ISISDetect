{
    "manifest_version": 2,
    "name": "ISIS Detect",
    "description": "Overlay twitter with terror risk information.",
    "version": "0.1",
    "icons": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
    "content_scripts": [
      {
        "matches": [
          "*://twitter.com/*"
        ],
        "js": ["lib/jquery.min.js", "src/main.js"],
        "run_at": "document_end"
      }
    ]
}
